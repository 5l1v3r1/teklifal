<ul class="nav nav-tabs mb-5">
  <li class="nav-item">
    <h2 class="h3 mr-5"><%= title %></h2>
  </li>

  <%#- if controller_name != 'sessions' %>
    <li class="nav-item">
      <% if params[:ann_created] %>
        <%= nav_link t('authentication.login'), new_session_path(resource_name, ann_created: true, content_type: params[:content_type]) %>
      <% else %>
        <%= nav_link t('authentication.login'), new_session_path(resource_name) %>
      <% end %>
      <%#= link_to "Log in", new_session_path(resource_name), class: 'nav-link' %>
    </li>
  <%# end -%>

  <%- if devise_mapping.registerable? %>
    <li class="nav-item">
      <% if params[:ann_created] or @ann %>
        <%= nav_link t('authentication.signup'), new_registration_path(resource_name, ann_created: true, content_type: params[:content_type]) %>
      <% else %>
        <%= nav_link t('authentication.signup'), new_registration_path(resource_name) %>
      <% end %>
      <%#= link_to "Sign up", new_registration_path(resource_name), class: "nav-link" %>
    </li>
  <% end -%>

</ul>